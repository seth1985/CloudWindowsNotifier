{
  "Environment": {
    "Mode": "ProductionCloud"
  },
  "Jwt": {
    "Issuer": "WindowsNotifierCloud",
    "Audience": "WindowsNotifierCloud",
    "Key": "change-this-prod-key",
    "ExpiryMinutes": 120
  },
  "Storage": {
    "Root": "%LOCALAPPDATA%\\\\CloudNotifier\\\\ApiStorage",
    "DevCoreModulesRoot": null,
    "Retention": {
      "RetainExportVersions": 3,
      "MaxExportAgeDays": 30,
      "MaxZipAgeDays": 30,
      "PruneOrphans": true
    }
  },
  "Telemetry": {
    "ApiKey": "replace-with-strong-prod-telemetry-key"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "Default": "Server=YOUR_SQL_SERVER;Database=WindowsNotifierCloud;Trusted_Connection=True;TrustServerCertificate=True"
  }
}
