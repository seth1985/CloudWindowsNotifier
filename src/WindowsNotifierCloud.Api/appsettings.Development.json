{
  "Environment": {
    "Mode": "DevelopmentLocal"
  },
  "Jwt": {
    "Issuer": "WindowsNotifierCloud",
    "Audience": "WindowsNotifierCloud",
    "Key": "change-this-dev-key-to-a-strong-value",
    "ExpiryMinutes": 120
  },
  "Storage": {
    "Root": "%LOCALAPPDATA%\\\\CloudNotifier\\\\ApiStorage",
    "DevCoreModulesRoot": "%LOCALAPPDATA%\\\\CloudNotifier\\\\Modules",
    "Retention": {
      "RetainExportVersions": 3,
      "MaxExportAgeDays": 30,
      "MaxZipAgeDays": 30,
      "PruneOrphans": true
    }
  },
  "Telemetry": {
    "ApiKey": "dev-telemetry-key-change-me"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  }
}
